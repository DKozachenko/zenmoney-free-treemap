{
  "name": "Zenmoney Free Treemap",
  "description": "Shows treemap in Zenmoney reports without modals",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "pages/popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://zenmoney.ru/a/*"
      ],
      "include_globs" :   ["*/#reports/treemap"]
    }
  ],
  "permissions": [],
  "background": {}
}